{
    "name": "clinical-quality-language",
    "displayName": "Clinical Quality Language - (CQL)",
    "description": "Syntax highlighting for the Clinical Quality Language (CQL)",
    "version": "2.0.4",
    "publisher": "jonnokc",
    "author": "Jonathan Adams",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Jonnokc/Clinical-Quality-Language/issues"
    },
    "icon": "assets/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Jonnokc/Clinical-Quality-Language"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cql",
                "aliases": [
                    "cql"
                ],
                "extensions": [
                    ".cql"
                ],
                "configuration": "./cql.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cql",
                "scopeName": "source.cql",
                "path": "./syntaxes/cql.language.json"
            }
        ]
    },
    "dependencies": {}
}
